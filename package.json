{
    "name": "edna-moda",
    "version": "3.0.0",
    "main": "node build",
    "scripts": {
        "start": "node app",
        "build": "node build",
        "test": "mocha",
        "lint": "eslint . --config .eslintrc.json --cache --ext .js,.ts",
        "full-lint": "eslint . --config .eslintrc.json --cache --cache-location .eslintfullcache --ext .js,.ts"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run lint"
        }
    },
    "mocha": {
        "diff": true,
        "extension": [
            "js"
        ],
        "reporter": "spec",
        "slow": 75,
        "timeout": 2000
    },
    "author": {
        "name": "Aldair Beltran",
        "email": "aldairbeltran3@hotmail.com",
        "url": "https://github.com/villager"
    },
    "dependencies": {
        "discord.js": "^12.2.0",
        "ps-data": "git://github.com/spaceshowdown/ps-data.git",
        "request": "^2.88.2",
        "sockjs-client": "^1.1.4"
    },
    "optionalDependencies": {
        "githubhook": "^1.9.3",
        "node-static": "^0.7.11"
    },
    "devDependencies": {
        "mocha": "^7.1.1",
        "husky": "^4.2.3",
        "eslint": "^7.1.0"
    },
    "url": "https://github.com/villager/Edna-Moda",
    "license": "MIT",
    "private": true
}
